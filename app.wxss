page {
    font-family: -apple-system-font, 'Helvetica Neue', Helvetica, 'Microsoft YaHei', sans-serif;
    background-color: #f5f5f5;
    display: flex;
    flex-direction: column;
    height: 100%;
    position: relative;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 300rpx 0 0 0;
  box-sizing: border-box;
  background: rgba(205, 198, 197, 1);
}
.bold {
  font-weight: bold;
} 
.cf{
    overflow:hidden; 
     clear: both;  
}

.ft10{
    font-size:10px;
}

.fl{
    float:left;
}

.fr{
    float:right;
}

.tips {
    font-size: 18px;
    text-align: center;
    padding: 50px;
    color: #ccc;
    font-weight:100;
}

.tips image {
    width: 30px;
    height: 30px;
    margin-right: 10px;
}

.tips image,
.tips text {
    vertical-align: middle;
}

.hide{
    display:none!important;
}

.show{
    display:block;
}
@import "common/common.wxss"